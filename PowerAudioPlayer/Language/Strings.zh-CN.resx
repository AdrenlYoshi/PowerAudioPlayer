  <data name="HTMLPlaylistTemplate" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html&gt;

&lt;head&gt;
    &lt;meta charset="UTF-8" /&gt;
    &lt;title&gt;{PlaylistName} - PowerAudioPlayer HTML 播放列表&lt;/title&gt;
    &lt;style&gt;
        body {
            background-color: #d7e5f3;
            font-size: 14px;
        }

        h1 {
            color: #002c6c;
        }

        hr {
            border: 1px solid orange;
        }

        .plContent {
            padding-left: 50px;
        }

        .plShow {
            border: 1px solid black;
        }

        .tipsText {
            color: #398fec;
        }

        .highLight {
            color: orange;
        }
    &lt;/style&gt;
    &lt;script&gt;
        function ReplaceURL() {
            document.getElementById("rc").setAttribute("href", window.location.href);
        }
    &lt;/script&gt;
&lt;/head&gt;

&lt;body onload="ReplaceURL()"&gt;
    &lt;h1&gt;PowerAudioPlayer 播放列表&lt;/h1&gt;
    &lt;p class="tipsText"&gt;有&lt;span class="highLight"&gt;{Count}&lt;/span&gt;个文件，总时长&lt;span
            class="highLight"&gt;{TotalLength}&lt;/span&gt;&lt;br /&gt;&lt;a id="rc" href=""&gt;右键单击以保存此HTML文件&lt;/a&gt;&lt;/p&gt;
    &lt;hr /&gt;
    &lt;div class="plShow"&gt;
        {PlaylistName} 的内容：
        &lt;div class="plContent"&gt;
            {PlaylistContent}
        &lt;/div&gt;
    &lt;/div&gt;
    &lt;hr /&gt;
    &lt;p class="tipsText"&gt;于&lt;span class="highLight"&gt;{GenerateTime}&lt;/span&gt;生成&lt;/p&gt;
&lt;/body&gt;

&lt;/html&gt;</value>
